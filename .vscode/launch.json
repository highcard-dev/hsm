{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug login",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "login",
                "--session-location=./session.json"
            ],
            "env": {},
            "showLog": true
        },
        {
            "name": "Debug serve",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "serve"
            ],
            "env": {},
            "showLog": true
        },
        {
            "name": "Debug update print",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "update",
                "--print-only"
            ],
            "env": {},
            "showLog": true
        },
        {
            "name": "Debug download",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "download",
                "--session-location=./session.json"
            ],
            "env": {},
            "showLog": true
        },
        {
            "name": "Debug start",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "start",
            ],
            "env": {},
            "showLog": true
        },
        {
            "name": "Debug serve with jwks",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "serve",
                "--jwks-endpoint=http://localhost:8081/jwks.json"
            ],
            "env": {},
            "showLog": true
        }
    ]
}